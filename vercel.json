{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_TWITCH_CLIENT_ID": "@vite_twitch_client_id",
    "VITE_TWITCH_CLIENT_SECRET": "@vite_twitch_client_secret"
  }
}
